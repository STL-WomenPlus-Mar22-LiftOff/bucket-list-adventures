<div class="container">
    <div class="row">
        @foreach (var activity in ViewBag.activitiesObject)
        {
            <div class="col-12 col-md-4 col-lg-3 search-result-container">
                <div class="search-result">
                    <a href="@Url.Action("Details","Home")?activity=@activity.name">  @activity.name </a>
                </div>
            </div>
        }
    </div>
</div>
    
